# API Configuration
ALPACA_API_KEY=your_api_key_here
ALPACA_SECRET_KEY=your_secret_key_here
ALPACA_BASE_URL=https://paper-api.alpaca.markets

# Model Parameters
LSTM_SEQUENCE_LENGTH=60
LSTM_UNITS=50
LSTM_DROPOUT=0.2
LSTM_EPOCHS=100
LSTM_BATCH_SIZE=32

# ARIMA Parameters
ARIMA_MAX_P=5
ARIMA_MAX_D=2
ARIMA_MAX_Q=5

# Data Parameters
SYMBOL=NVDL
LOOKBACK_YEARS=2
TEST_SIZE=0.2

# Trading Parameters
INITIAL_CAPITAL=10000
RISK_FREE_RATE=0.02

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
LOG_FILE=nvdl_predictor.log