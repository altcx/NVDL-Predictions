LSTM_DROPOUT=0.2
LSTM_EPOCHS=100
LSTM_BATCH_SIZE=32

# ARIMA Model Configuration
ARIMA_MAX_P=5
ARIMA_MAX_D=2
ARIMA_MAX_Q=5